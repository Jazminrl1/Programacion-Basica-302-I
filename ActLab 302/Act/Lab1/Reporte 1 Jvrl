        "//PB302_1.cpp\n",
        "#include <iostream>\n",
        "using namespace std;\n",
        "int main()\n",
        "{\n",
        "  char nombre[10]=\"Jazmin\";\n",
        "  char paterno[10]=\"Renteria\";\n",
        "  char materno[10]=\"Lopez\";\n",
        "  int edad=17;\n",
        "  char direccion[40]=\"calle cadereyta\";\n",
        "  char fechanac[11]=\"16/7/2008\";\n",
        "  char ciudadnac[10]=\"Sonora\";\n",
        "  cout<<\"Tu nombre completo es:\\n\";\n",
        "  cout<<nombre<<\"\\n\";\n",
        "  cout<<paterno<<\"\\n\";\n",
        "  cout<<materno<<\"\\n\";\n",
        "  cout<<edad<<\"\\n\";\n",
        "  cout<<direccion<<\"\\n\";\n",
        "  cout<<fechanac<<\"\\n\";\n",
        "  cout<<ciudadnac<<\"\\n\" ;\n",
        "\n",
        "  return 0;\n",
        "\n",
        "}\n"
